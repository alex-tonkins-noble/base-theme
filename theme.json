			"postTypes": [ "page" ]
		},
		{
			"name": "page-sidebar-left",
			"title": "Page: Two columns, left sidebar",
			"postTypes": [ "page" ]
		},
		{
			"name": "page-template-patterns",
			"title": "Template for block pattern layouts",
			"postTypes": [ "page", "post" ]
		}
	],
	"patterns": [ "heading-with-two-font-sizes-and-separators" ]
