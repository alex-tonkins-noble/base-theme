{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"useRootPaddingAwareAlignments": false,
		"layout": {
			"contentSize": "var(--np-regular-size)",
			"wideSize": "var(--np-wide-size)"
		},
		"spacing": {
			"margin": false,
			"padding": false,
			"units": [ "px", "em" ],
			"blockGap": true
		},
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "var(--np-primary)",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "var(--np-secondary)",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "var(--np-tertiary)",
					"name": "Tertiary"
				},
				{
					"slug": "black",
					"color": "var(--np-black)",
					"name": "Black"
				},
				{
					"slug": "white",
					"color": "var(--np-white)",
					"name": "White"
				},
				{
					"slug": "light-grey",
					"color": "var(--np-grey)",
					"name": "Light grey"
				}
			],
			"duotone": [],
			"gradients": [],
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"link": true
		},
		"typography": {
			"fontSizes": [],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"customFontSize": false,
			"lineHeight": false,
			"textDecoration": false,
			"dropCap": false,
			"textTransform": false,
