	"styles": {
		"color": {
			"background": "var(--np-white)",
			"text": "var(--np-black)"
		},
		"typography": {
			"fontSize": "16px",
			"fontFamily": "Poppins",
			"lineHeight": "1.4"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--np-tertiary)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--np-secondary)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			},
			"button": {
				"border": {
					"width": "0px",
					"radius": "0px"
				}
			}
		},
		"blocks": {
			"core/button": {
				"border": {
					"width": "0px",
					"radius": "4px"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "main",
			"title": "Main",
			"area": "general"
		}
	],
	"customTemplates": [
		{
			"name": "post-sidebar-right",
			"title": "Post: Two columns, right sidebar",
			"postTypes": [ "post" ]
		},
		{
			"name": "post-sidebar-left",
			"title": "Post: Two columns, left sidebar",
			"postTypes": [ "post" ]
		},
		{
			"name": "page-sidebar-right",
